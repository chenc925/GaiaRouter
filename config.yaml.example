# GaiaRouter 配置文件示例

# 数据库配置（阿里云RDS）
database:
  host: your-rds-host.rds.aliyuncs.com
  port: 3306
  user: your_username
  password: your_password
  database: gaiarouter
  pool_size: 10
  max_overflow: 20

# 提供商API Key配置
providers:
  openai_api_key: ${OPENAI_API_KEY}
  anthropic_api_key: ${ANTHROPIC_API_KEY}
  google_api_key: ${GOOGLE_API_KEY}
  openrouter_api_key: ${OPENROUTER_API_KEY}

# 服务器配置
server:
  host: 0.0.0.0
  port: 8000
  debug: false
  log_level: INFO

# 请求配置
request_timeout: 60
max_retries: 3

# 模型配置
models:
  - id: openai/gpt-4
    provider: openai
    name: gpt-4
    api_key_env: OPENAI_API_KEY
  - id: openai/gpt-3.5-turbo
    provider: openai
    name: gpt-3.5-turbo
    api_key_env: OPENAI_API_KEY
  - id: anthropic/claude-3-opus
    provider: anthropic
    name: claude-3-opus-20240229
    api_key_env: ANTHROPIC_API_KEY
  - id: google/gemini-pro
    provider: google
    name: gemini-pro
    api_key_env: GOOGLE_API_KEY
  - id: openrouter/gpt-4
    provider: openrouter
    name: openai/gpt-4
    api_key_env: OPENROUTER_API_KEY

